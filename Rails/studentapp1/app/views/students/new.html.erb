<%= form_for :student, url: students_path do |s| %>
<p>
	<%=s.label :first_name %><br>
	<%= s.text_field :first_name%>
</p>

<p>
	<%=s.label :last_name %><br>
	<%= s.text_field :last_name%>
</p>

<p>
	<%=s.label :age%><br>
	<%= s.number_field :age%>
</p>
<p>
	<%= s.submit %>
</p>

<% end %>
<%= link_to 'Back', students_path %>
