<div class="col-sm-7">
	 <%=image_tag("tweet.jpg", height: '450px', width: '650px') %>
</div>
<div class="col-sm-5">
<ul>
	<% if user_signed_in? %>
	<li>
	<%= link_to "Sign Out", destroy_user_session_path, method: :delete %>
	</li>
	<li>
		<%= current_user.email%>
	</li>
	<li>
		<%= link_to "My profile", post_path(current_user)%>
	</li>
	<% else %>
	<li>
		<%= link_to "Sign In", new_user_session_path%>
	</li>
	<li>
		<%= link_to "Sign Up", new_user_registration_path%>
	</li>
	
	<% end %>
</ul>
<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>
</div>







